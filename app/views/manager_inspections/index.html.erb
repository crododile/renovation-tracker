<h1>Listing manager_inspections</h1>

<table>
  <thead>
    <tr>
      <th>Components</th>
      <th>New countertop</th>
      <th>Flooring</th>
      <th>Appliances</th>
      <th>Paint</th>
      <th>Rent ready</th>
      <th>Property</th>
      <th>Unit number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manager_inspections.each do |manager_inspection| %>
      <tr>
        <td><%= manager_inspection.components %></td>
        <td><%= manager_inspection.new_countertop %></td>
        <td><%= manager_inspection.flooring %></td>
        <td><%= manager_inspection.appliances %></td>
        <td><%= manager_inspection.paint %></td>
        <td><%= manager_inspection.rent_ready %></td>
        <td><%= manager_inspection.property %></td>
        <td><%= manager_inspection.unit_number %></td>
        <td><%= link_to 'Show', manager_inspection %></td>
        <td><%= link_to 'Edit', edit_manager_inspection_path(manager_inspection) %></td>
        <td><%= link_to 'Destroy', manager_inspection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manager inspection', new_manager_inspection_path %>
